<%- include('../common/header') %>
<%- include('../common/menu')  %>

<script>window.document.title = `Detalhe do <%= diverso.titulo %>`</script>
     
<main>
    <div class="detalhe">
        <div class="detalhe__titulo"><%= diverso.titulo %></div>
        <div class="detalhe__imgDesc">
            <img class="detalhe__imagem" src="<%= diverso.imagem %>" alt="<%= diverso.alt %>">
            <p class="detalhe__desc"><%= diverso.desc %></p>
        </div>
        <p class="detalhe__valor"><%= diverso.valor %></p>
    </div>
</main>
<script src="js/detalhe.js"></script>
<%- include('../common/footer')  %>