<%- include('../common/header') %>
<%- include('../common/menu')  %>

<script>window.document.title = `Detalhe do <%= star.titulo %>`</script>
     
<main>
    <div class="detalhe">
        <div class="detalhe__titulo"><%= star.titulo %></div>
        <div class="detalhe__imgDesc">
            <img class="detalhe__imagem" src="<%= star.imagem %>" alt="<%= star.alt %>">
            <p class="detalhe__desc"><%= star.desc %></p>
        </div>
        <p class="detalhe__valor"><%= star.valor %></p>
    </div>
</main>
<script src="js/detalhe.js"></script>
<%- include('../common/footer')  %>